<!DOCTYPE html>
<html>
<head>
    <!-- Required meta tags -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Integrity Builders & Design Inc.</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="it">
    <meta name="keywords" content="Rapoo,creative, agency, startup, Mobicon,onepage, clean, modern,business, company,it">
    <meta name="author" content="Dreambuzz">

    <!-- Plugins CSS -->
    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/css/Pe-icon-7-stroke.css">

    <link rel="stylesheet" href="~/Content/css/slick.css">
    <link rel="stylesheet" href="~/Content/css/slick-theme.css">
    <link rel="stylesheet" href="~/Content/css/all.css">

    <!-- Theme CSS -->
    <link rel="stylesheet" href="~/Content/css/style.css">
    <link rel="stylesheet" href="~/Content/css/responsive.css">

    <!-- JAVASCRIPT
    ================================================== -->
    <!-- Global JS -->
    <script src="~/Content/js/jquery.min.js"></script>
    <script src="~/Content/js/popper.min.js"></script>

    <!-- Plugins JS -->
    <script src="~/Content/js/bootstrap.min.js"></script>

    <!-- Slick JS -->
    <script src="~/Content/js/jquery.easing.1.3.js"></script>
    <script src="~/Content/js/slick.min.js"></script>
    <!-- Theme JS -->
    <script src="~/Content/js/theme.js"></script>
</head>
<body class="top-header" id="top-body">
    <!-- LOADER TEMPLATE -->
    <div id="page-loader">
        <div class="loader-icon fa fa-spin colored-border"></div>
    </div>
    <!-- /LOADER TEMPLATE -->
    <!-- NAVBAR
       ================================================= -->
    <nav class="navbar navbar-expand-lg navbar-dark trans-navigation fixed-top navbar-togglable">
        <div class="container">
            <div class="col-lg-3 logo-container">
                <div class="logo d-none d-lg-block">
                    <!-- Brand -->
                    <a class="navbar-brand" href="@Url.Action("Index", "Home")">
                        <h1><img src="~/Content/img/custom/Logo_horizontal.png" alt="logo-img" class="img-fluid"></h1>
                    </a>
                </div>
            </div>
            <!-- Toggler -->
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
                <span class="fa fa-bars"></span>
            </button>

            <!-- Collapse -->
            <div class="collapse navbar-collapse" id="navbarCollapse">
                <!-- Links -->
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item ">
                        <a href="@Url.Action("Index", "Home")" class="nav-link js-scroll-trigger">
                            Home
                        </a>
                    </li>
                    <li class="nav-item ">
                        <a href="@Url.Action("Index", "Service")" class="nav-link js-scroll-trigger">
                            Services
                        </a>
                    </li>
                    <li class="nav-item ">
                        <a href="@Url.Action("Index", "Team")" class="nav-link js-scroll-trigger">
                            Team
                        </a>
                    </li>
                    <li class="nav-item ">
                        <a href="@Url.Action("Index", "Gallery")" class="nav-link js-scroll-trigger">
                            Gallery
                        </a>
                    </li>
                    <li class="nav-item ">
                        <a href="@Url.Action("Index", "Testimonial")" class="nav-link js-scroll-trigger">
                            Testimonials
                        </a>
                    </li>
                    <li class="nav-item ">
                        <a href="@Url.Action("Index", "Contact")" class="nav-link">
                            Contact
                        </a>
                    </li>
                </ul>
            </div> <!-- / .navbar-collapse -->
        </div> <!-- / .container -->
    </nav>

    @RenderBody()

    <!-- FOOTER
       ================================================== -->
    <footer class="section " id="footer">
        <div class="overlay footer-overlay"></div>
        <!--Content -->
        <div class="container">
            <div class="row justify-content-start">
                <div class="col-lg-4 col-sm-12">
                    <div class="footer-widget">
                        <!-- Brand -->
                        <a href="#" class="footer-brand text-white">
                            Integrity Builders & Design
                        </a>
                        <p>Each theme featured at the Bootstrap marketplace has been reviewed by Bootstrap's creators.Lorem ipsum dolor sit amet, consectetur adipisicing elit.</p>
                    </div>
                </div>

                <div class="col-lg-3 ml-lg-auto col-sm-12">
                    <div class="footer-widget">
                        <h3>Account</h3>
                        <!-- Links -->
                        <ul class="footer-links ">
                            <li>
                                <a href="@Url.Action("Index", "Home")" class="nav-link js-scroll-trigger">
                                    Home
                                </a>
                            </li>
                            <li >
                                <a href="@Url.Action("Index", "Service")" class="nav-link js-scroll-trigger">
                                    Services
                                </a>
                            </li>
                            <li>
                                <a href="@Url.Action("Index", "Team")" class="nav-link js-scroll-trigger">
                                    Team
                                </a>
                            </li>
                            <li>
                                <a href="@Url.Action("Index", "Gallery")" class="nav-link js-scroll-trigger">
                                    Gallery
                                </a>
                            </li>
                            <li>
                                <a href="@Url.Action("Index", "Testimonial")" class="nav-link js-scroll-trigger">
                                    Testimonials
                                </a>
                            </li>
                            <li>
                                <a href="@Url.Action("Index", "Contact")" class="nav-link">
                                    Contact
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>

                <div class="col-lg-2 col-sm-6">
                    <div class="footer-widget">
                        <h3>Socials</h3>
                        <!-- Links -->
                        <ul class="list-unstyled footer-links">
                            <li><a href="#"><i class="fab fa-facebook-f"></i>Facebook</a></li>
                            <li>
                                <a href="#">
                                    <i class="fab fa-twitter"></i>Twitter
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    <i class="fab fa-pinterest-p"></i>Pinterest
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    <i class="fab fa-linkedin"></i>linkedin
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    <i class="fab fa-youtube"></i>YouTube
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div> <!-- / .row -->
        </div> <!-- / .container -->
    </footer>


    <!--  Page Scroll to Top  -->
    <a class="scroll-to-top js-scroll-trigger" href="#top-body">
        <i class="fa fa-angle-up"></i>
    </a>


</body>
</html>
